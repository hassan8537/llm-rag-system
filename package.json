{
  "name": "motor-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "dev": "nodemon",
    "dev:ts": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "db:init": "ts-node src/scripts/init-db.ts",
    "db:seed": "ts-node src/scripts/seed-db.ts",
    "docker:build": "docker build -t motor-backend:latest .",
    "docker:run": "docker run -p 3000:3000 --env-file .env motor-backend:latest",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.857.0",
    "@aws-sdk/s3-request-presigner": "^3.857.0",
    "@types/pdf-parse": "^1.1.5",
    "aws": "^0.0.3-2",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "commander": "^13.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "jsonwebtoken": "^9.0.2",
    "openai": "^5.11.0",
    "pdf-parse": "^1.1.1",
    "pdf-parser": "^1.0.5",
    "pg": "^8.16.3",
    "pgvector": "^0.2.1",
    "postgres": "^3.4.7",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.1",
    "sequelize-typescript": "^2.1.6",
    "umzug": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.6",
    "@types/commander": "^2.12.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.5",
    "@types/sequelize": "^4.28.20",
    "@types/umzug": "^2.3.9",
    "nodemon": "^3.1.10",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
